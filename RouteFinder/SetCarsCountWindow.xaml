<Window x:Class="RouteFinder.SetCarsCountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RouteFinder"
        mc:Ignorable="d" 
        Title="Количество машин" Height="77.219" Width="377.218">
    <Grid Margin="0,0,2,-2">
        <Label x:Name="label" Content="Введите количество доступных машин:" HorizontalAlignment="Left" Margin="5,5,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="CarsCountBox" HorizontalAlignment="Left" Margin="236,10,0,0" VerticalAlignment="Top" Width="42"/>
        <Button x:Name="SetCarsCountButton" Content="Задать" HorizontalAlignment="Left" Margin="283,10,0,0" VerticalAlignment="Top" Width="75" Height="18" FontSize="10" Click="SetCarsCountButton_Click"/>

    </Grid>
</Window>
